Q6. Time to equality


public class Solution {
    public int solve(int[] A) {
        int max=Integer.MIN_VALUE;
        int times=0;
        for(int i=0;i<A.length;i++){
            if(A[i]>max){
                max=A[i];
            }
        }
        for(int i=0;i<A.length;i++){
            times+=max-A[i];
           

        }
        return times;
    }
}

